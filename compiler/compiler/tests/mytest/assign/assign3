DECLARE
    a(0:10)
BEGIN
    a(0) := 0;
    a(1) := 10;
    a(2) := 100;
    a(3) := 1000;
    a(4) := 10000;
    a(5) := 100000;
    a(6) := 1000000;
    a(7) := 10000000;
    a(8) := 100000000;
    a(9) := 1000000000;
    a(10) := 10000000000;

    WRITE a(0);
    WRITE a(1);
    WRITE a(2);
    WRITE a(3);
    WRITE a(4);
    WRITE a(5);
    WRITE a(6);
    WRITE a(7);
    WRITE a(8);
    WRITE a(9);
    WRITE a(10);
END