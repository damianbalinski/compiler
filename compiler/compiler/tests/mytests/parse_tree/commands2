DECLARE
    a
BEGIN
    IF 10 > 20 THEN
        WRITE 100;
        READ a;
        a := 200;
        b := 300;
        b := 20 + 20;
        b := 20 - 20;
        b := 20 * 20;
        b := 20 / 20;
        b := 20 % 20;
    ENDIF

    IF 10 > 20 THEN
        WRITE 20;
        WRITE 30;
        READ a;
    ELSE
        WRITE 30;
        WRITE 40;
    ENDIF
END