DECLARE
	a, b, i
BEGIN
    
    READ i;
	WHILE i <= 10 DO
  		i := i + 1;
	ENDWHILE

    b := 20;
	FOR ii FROM 20 DOWNTO 10 DO
        WRITE ii;
	ENDFOR
END

